proj = ims

make:
	@g++ -Wall -std=c++17 $(proj).cpp -o $(proj)

2a:
	@./ims -d 4,12,22,28,38 -r 1,2,3,4,5 -e 50 -c 1

2b:
	@./ims -d 4,12,22,28,38 -r 1,2,3,4,5 -e 50 -c 0

2c:
	@./ims -d 4,12,22,28,38 -r 1,2,3,4,5 -e 350 -c 1

2d:
	@./ims -d 4,12,22,28,38 -r 1,2,3,4,5 -e 350 -c 0

